<!DOCTYPE html>
<html lang="en">
<%= render "users/header" %>
<body class="push-body html front not-logged-in no-sidebars page-games">
    <div id="main">
        <div id="main_inner">
            <div class="region region-content">
                <div class="recent-games__wrapperr" style="min-height:0px"></div>
            </div>
            <div id="block-system-main" class="block block-system block-odd first last clearfix">
                <div class="panel-display panel-1col clearfix">
                    <div class="panel-panel panel-col">
                        <div>
                            <div class="panel-pane pane-custom pane-1">
                                <div class="pane-content">
                                    <div style="clear:both;"></div>
                                </div>
                            </div>
                            <div class="panel-separator"></div>
                            <div class="addiction-game-sec panel-pane pane-views pane-index-page-games-sections">
                                <div class="container">
                                    <div class="index-page-games-section ">
                                        <div class="top-head">
                                            <h2 class="new-games-head">
                                                Game Má»›i
                                            </h2>
                                        </div>
                                        <div class="game-gallerty inn_panel1_box stra_games">
                                            <div class="row">
                                            <% @games.each do |game| %>       
                                                <div class="col-lg-2 col-md-2 col-sm-2 inn_box_panel1">
                                                    <div class="inn_box_panel1_box hasVideo">
                                                        <a data-id="<%= game.id %>" href="" class="pic_box">
                                                            <div class="thumbnail-image mobile-enabled">
                                                                <div class="b-loader">
                                                                   <img data-id="<%= game.id %>" class="b-lazy b-loaded" src="<%= game.imggame %>" alt="">
                                                                </div>
                                                            </div>
                                                        </a>
                                                        <h4 style="display:flex" class="txt_box my-h4">
                                                            <div class="">
                                                                <a  href="/game/<%= game.id %>">
                                                                    <%= game.namegame %>
                                                                </a>
                                                            </div>
                                                        </h4>
                                                    </div>
                                                </div>
                                            <% end %>    
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-separator"></div>
                            <div class="addiction-game-sec panel-pane pane-views pane-index-page-games-sections">
                                <div class="container">
                                    <div class="index-page-games-section ">
                                        <div class="top-head">
                                            <h2 class="new-games-head">
                                                Game Hay
                                            </h2>
                                        </div>
                                        <div class="game-gallerty inn_panel1_box stra_games">
                                            <div class="row">
                                            <% @gamesrandom.each do |random| %>       
                                                <div class="col-lg-2 col-md-2 col-sm-2 inn_box_panel1">
                                                    <div class="inn_box_panel1_box hasVideo">
                                                        <a data-id="<%= random.id %>" href="" class="pic_box">
                                                            <div class="thumbnail-image mobile-enabled">
                                                                <div class="b-loader">
                                                                   <img data-id="<%= random.id %>" class="b-lazy b-loaded" src="<%= random.imggame %>" alt="">
                                                                </div>
                                                            </div>
                                                        </a>
                                                        <h4 style="display:flex" class="txt_box my-h4">
                                                            <div class="">
                                                                <a  href="#"><%= random.namegame %></a>
                                                            </div>
                                                        </h4>
                                                    </div>
                                                </div>
                                            <% end %>    
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="clear:both;"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>